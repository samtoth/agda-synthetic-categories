\author{samtoth}
\title{Testing Agda}

\xmlns:html{http://www.w3.org/1999/xhtml}

\def\codeblock[lang][body]{
  \<html:code>[class]{\lang highlight grace-loading}{\body}
}

\def\embed[src]{
  \<html:embed>[type]{text/html}[src]{\src}{}
}


\def\agda[x]{\codeblock{agda}{\x}}

% ```agda
\agda{

module test where

  data Blah : Set where
    a b : Blah


  bl : Blah → Blah
  bl a = b
  bl b = a

  bll : Blah → Blah
  bll = ?
}
% ```
