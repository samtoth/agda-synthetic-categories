\subtree[stt-002R]{
\title{Nat}
\taxon{Definition}

% ```agda
\agda{
data ℕ : Type where
  zero : ℕ
  suc : ℕ → ℕ


{-# BUILTIN NATURAL ℕ #-}


_+_ : ℕ → ℕ → ℕ
n + zero = n
n + suc m = suc (n + m)

+-zero : ∀ {n} → 0 + n ＝ n
+-zero {zero} = refl
+-zero {suc n} = ap suc +-zero

+-succ : ∀ {n m} → suc (n + m) ＝ suc n + m
+-succ {n} {zero} = refl
+-succ {n} {suc m} = ap suc +-succ

+-comm : ∀ {n m} → n + m ＝ m + n
+-comm {n} {zero} = sym +-zero
+-comm {n} {suc m} = ap suc (+-comm {n}) ∙ +-succ
}
% ```

}
