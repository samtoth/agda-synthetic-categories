\subtree[stt-002L]{
\taxon{theorem}
\title{Canonical pushouts are pushouts}

% ```agda
\agda{
Pushout-is-pushout : ∀ {𝓤 𝓥 𝓦} {A : Type 𝓤} {B : Type 𝓥}
                       {C : Type 𝓦} {f : A → B} {g : A → C}
                     → is-pushout (mk-span _ f g) pushout
Pushout-is-pushout {f = f} {g} = is-equiv←qiso iso where
  iso : quasi-iso (cocone-map (mk-span _ f g) pushout)
  iso .fst = pushout-rec
  iso .snd .fst f = ext! (pushout-ind _ λ where
    .CoconeD.p _ → refl
    .CoconeD.q _ → refl
    .CoconeD.filler a → ap (λ a → coe a refl) pre-post-comp ∙
                        sym (coe∙coe (ap _ (glue a) ∙-) _ refl) ∙
                        coe-precomp＝ _ (ap f (glue a)) ∙
                        ap (_∙ ap f (glue a)) (coe-postcomp＝ _ refl ∙ ∙-reflr _) ∙
                        ∙-diff-zero' {p = ap _ (glue a)} {q = ap f (glue a)} (pushout-rec-apβ a))
  iso .snd .snd (mk-cocone p q filler)
    = ap (mk-cocone p q) (ext! pushout-rec-apβ)


}
% ```

}
